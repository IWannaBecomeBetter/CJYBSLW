<!-- 操作记录 -->
<div class="modal fade operationRecord" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-hidden="true" style="display: none;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="myLargeModalLabel">操作记录</h4>
            </div>
            <div class="modal-body">
                <table id="operationTable" class="table table-striped">
                    <input type="hidden" name="orderId">
                </table>
            </div>
            #parse('modal/modalFooterClose.vm')
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
